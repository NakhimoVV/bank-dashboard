{
  "name": "bank-dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "minista",
    "build": "minista build && touch dist/.nojekyll",
    "preview": "minista preview",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "format": "prettier . --write",
    "lint": "npm run lint:js ; npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix && npm run format"
  },
  "prettier": {
    "semi": false
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "minista": "^3.1.12",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.91.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  }
}
